ton,\ntextarea,\nselect {\n  font: inherit;\n}\n\n/* Remove all animations and transitions for people that prefer not to see them */\n@media (prefers-reduced-motion: reduce) {\n  * {\n    animation-duration: 0.01ms !important;\n    animation-iteration-count: 1 !important;\n    transition-duration: 0.01ms !important;\n    scroll-behavior: auto !important;\n  }\n}","less/less/partials/typography.less":"html{ \n    font-size: 1px;/*for using REM units*/\n }\n body{\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;\n    font-size: 16rem;\n    line-height: 1.3;\n    color: #fff;\n }\n\n\n h1 {\n    font-size: 48rem;\n    font-weight: 600;\n }\n \n h2 {\n    font-size: 24rem;\n    font-weight: 400;\n }\n\n p {\n    color: @grey--lightest;\n    font-size: 15rem;\n    font-weight: 150;\n }\n\n .text__body {\n    margin-bottom: 25px;\n }\n\n .text__subtitle {\n    margin-bottom: 25px;\n    &--highlight {\n    color: @highlight;\n   }\n }\n\n ","less/less/partials/base.less":".container {\n    padding: 3em 2em;\n    scroll-snap-align: center;\n\n    &--fill {\n        background: @grey--light;\n    }\n    &--highlight {\n        border-top: 1px solid @highlight;\n    }\n} \n.ico {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    width: 4em;\n    height: 4em;\n    background: @highlight;\n    border-radius: 50%;\n    margin-bottom: 25px;\n}\n\n.logo {\n    display: inline-block;\n    margin-right: 15px;\n    margin-top: 15px;\n    height: 20px;\n    width: 20px;\n}","less/less/partials/grid.less":"body {\n    display: grid;\n    grid-template-columns: minmax(250px, 1250px);\n    grid-template-rows: minmax(min-content, max-content);\n    justify-content: center;\n    grid-gap: 150px;\n    background: @grey--dark;\n}\n\n.row {\n    display: grid;\n    grid-gap: 25px;\n    align-items: center;\n    justify-content: space-between;\n    &--logo {\n        justify-content: center;\n        align-items: center;\n    }\n    &--text {\n        border-top: 1px solid @grey--light;\n        padding-top: 64rem;\n        grid-template-rows: minmax(10px, max-content);\n        grid-template-columns: 1fr;\n        text-align: center;\n    }\n    &--2 {\n        grid-template-columns: repeat(2, minmax(min-content, 1fr));\n        grid-template-rows: auto;\n        padding: 0px;\n    }\n    @media screen and (max-width: 800px) {\n        //text-align: center;\n        &--2 {\n            grid-template-columns: repeat(4, minmax(100%, max-content));\n            overflow-x: scroll;\n            scroll-snap-type: x mandatory;\n        }\n    }\n}\n","less/less/partials/inputs.less":"form {\n    width: 100%;\n}\n\n.input {\n    outline: 1px solid @white;\n    border: none;\n    background: none;\n    width: 100%;\n    height: 64rem;\n    padding: 0 25px;\n    transition: outline 275ms linear;\n    margin-bottom: 25px;\n    &:focus {\n        outline: 1px solid @highlight;\n    }\n}","less/less/partials/buttons.less":".btn {\n    width: 100%;\n    height: 64rem;\n    background: none;\n    border: none;\n    -webkit-appearance: none;\n\n    &--primary {\n        background: @white;\n    }\n}"},"lineCount":null}},"error":null,"hash":"0805a8d7f475e0507e4e1b4b6d2cfaa2","cacheData":{"env":{}}}